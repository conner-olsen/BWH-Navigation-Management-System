{
  "type": "module",
  "private": true,
  "name": "team-d-production",
  "version": "1.0.0",
  "description": "",
  "license": "UNLICENSED",
  "workspaces": [
    "apps/*",
    "configs/*",
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "fix": "node scripts/fixes/fix.js",
    "fix:docker": "node scripts/fixes/fix.js docker",
    "fix:general": "node scripts/fixes/fix.js general",
    "dev": "docker-compose -f docker-compose.dev.yaml up --build --renew-anon-volumes -d",
    "dev:stop": "docker-compose -f docker-compose.dev.yaml down --volumes --rmi local",
    "deploy": "docker-compose -f docker-compose.prod.yaml up --build -d",
    "deploy:stop": "docker-compose -f docker-compose.prod.yaml down",
    "sync": "git add . && git commit -m \"Auto-commit before merge | $(git rev-parse --abbrev-ref HEAD) - $(date +'%Y-%m-%d %I:%M%p')\" || true && git fetch && git push --set-upstream origin $(git rev-parse --abbrev-ref HEAD) && git fetch origin main:main && git merge -m 'Synced with main' FETCH_HEAD && git push",
    "test": "docker-compose -f docker-compose.test.yaml up --build --renew-anon-volumes -d",
    "test:stop": "docker-compose -f docker-compose.dev.yaml down --volumes --rmi local",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "postinstall": "husky install || exit 0",
    "build": "turbo run build",
    "docker:upload": "turbo run docker:upload --concurrency=1",
    "depcheck": "depcheck",
    "setup": "yarn install && yarn build:dev"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/bootstrap": "^5",
    "@types/eslint": "^8",
    "@types/identity-obj-proxy": "^3",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1",
    "@types/node": "^20.11.20",
    "@types/react-dom": "^18.0.11",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-custom": "workspace:*",
    "happy-dom": "^10.6.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "prettier": "2.8.8",
    "prettier-config-custom": "workspace:*",
    "prisma": "^5.10.1",
    "ts-jest": "^29.1.2",
    "tsconfig-custom": "workspace:*",
    "turbo": "latest",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^5.10.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@yarnpkg/pnpify": "^4.0.1",
    "bootstrap": "^5.3.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.323.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-select": "^5.8.0"
  }
}
